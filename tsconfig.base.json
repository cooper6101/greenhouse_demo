{
    "compilerOptions": {
      "baseUrl": ".",
      "target": "ES2023",
      "module": "NodeNext",
      "moduleResolution": "NodeNext",
      "lib": [
        "ES2023"
      ],
      "types": [
        "node"
      ],
      "skipLibCheck": true,
      "sourceMap": true,
      "allowJs": false,
      "strict": true,
      "noImplicitThis": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "noImplicitReturns": true,
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "forceConsistentCasingInFileNames": true,
      /** Ones we'd like to remove but are too cumbersome now */
      "noImplicitAny": false,
      "useUnknownInCatchVariables": false
    },
  }